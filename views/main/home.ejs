        <span class="myRegisterNav">
          <span style="color:white; padding-left:20px"
            ><a style="color:white;" href="/api/users/register">Register</a>/<a
              style="color:white;"
              href="/api/users/login"
              >Login</a
            ></span
          >
        </span>
        <% } else{ %>
          <span style="color:orange" class="myNavGreeting" > hello: <%= user.profile.name %> <a href="/" style="color:orange"></a> <span/>
